public class control_if03 {

	public static void main(String[] args) {
		int score = 98;
		char grade;
		
		if(score >=90) {
			grade = 'A';
			System.out.println("아주 잘 했습니다");
		}
		else if(score >= 80) {
			grade = 'B';
		}
		else if(score >= 70) {
			grade = 'C';
		}
		else if(score >= 60) {
			grade = 'D';
		}
		else {
			grade = 'F';
		}
		if (grade == 'A') {
			System.out.println("아주 잘 했습니다");
		}
		System.out.println("당신의 등급은 "+grade+" 입니다");
	}

}
